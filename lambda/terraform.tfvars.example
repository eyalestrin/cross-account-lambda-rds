# AWS Configuration
aws_region = "us-east-1"

# RDS Account Information
rds_account_id = "YOUR_RDS_ACCOUNT_ID"  # REQUIRED: Replace with actual RDS AWS account ID

# Database Secret (obtain from RDS account after deployment)
db_secret_arn = "arn:aws:secretsmanager:us-east-1:123456789012:secret:rds-db-credentials-xxxxx"

# VPC Lattice Configuration (obtain from RDS account after deployment)
rds_vpc_lattice_service_arn = "arn:aws:vpc-lattice:us-east-1:123456789012:service/svc-xxxxx"
