# AWS Configuration
aws_region = "us-east-1"

# Lambda Account Information
lambda_account_id = "YOUR_LAMBDA_ACCOUNT_ID"  # REQUIRED: Replace with actual Lambda AWS account ID from step 1

# Database Configuration
db_name             = "transactions_db"
db_username         = "dbadmin"
db_instance_class   = "db.t3.micro"
allocated_storage   = 20

# VPC Lattice Configuration (obtain from Lambda account after deployment)
lambda_service_network_arn = "REPLACE_WITH_ACTUAL_SERVICE_NETWORK_ARN_FROM_STEP_1"
